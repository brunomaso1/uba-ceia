# Mongo DB
MONGODB_SERVER_HOST=localhost
MONGODB_SERVER_PORT=27017
MONGODB_INITDB_DATABASE=picudo-rojo
MONGODB_USER=<SECRET>
MONGODB_PASSWORD=<SECRET>

# Scaleway environment
SCALEWAY_BUCKET=picudo-rojo-bucket
