# Minio environment
MINIO_BUCKET=picudo-rojo-bucket
MINIO_ENDPOINT_URL=http://localhost:9000
MINIO_ACCESS_KEY=<SECRET>
MINIO_SECRET_KEY=<SECRET>
MINIO_REGION=nl-ams